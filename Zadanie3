
n = int(input("Введите номер месяца "))

months = {
          12: "Зима", 1: "Зима", 2: "Зима",
          3: "Весна", 4: "Весна", 5: "Весна",
          6: "Лето", 7: "Лето", 8: "Лето",
          9: "Осень", 10: "Осень", 11: " Осень"
          }


print(months.get(n))

if n > 12 or n == 0:

    print("Такого месяца не существует")

